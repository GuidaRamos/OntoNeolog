        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guida/ontologies/2025/4/Lexical_innovation#LinguisticUnit"/>
        <dc:description xml:lang="en">bound morpheme that is attached to a word stem to form a new word</dc:description>
        <dc:description xml:lang="pt">unidade não autónoma que se associa a uma forma de base</dc:description>
        <dc:description xml:lang="fr">élément non autonome qui s&apos;ajoute à la base du mot</dc:description>
        <rdfs:isDefinedBy xml:lang="en">https://eneoli.wikibase.cloud/wiki/Item:Q1116</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">affix</rdfs:label>
        <rdfs:label xml:lang="fr">affixe</rdfs:label>
        <rdfs:label xml:lang="pt">afixo</rdfs:label>
    </owl:Class>
    


